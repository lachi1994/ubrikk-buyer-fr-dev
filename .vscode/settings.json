{
	"css.lint.unknownAtRules": "ignore",
	"editor.wordWrap": "on",
	"editor.tabSize": 3,
	"editor.detectIndentation": true,
	"files.eol": "auto",
	"prettier.arrowParens": "always",
	"prettier.jsxBracketSameLine": true,
	"prettier.jsxSingleQuote": true,
